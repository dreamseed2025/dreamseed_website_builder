{
  "name": "dreamseed-platform",
  "version": "2.0.0",
  "description": "Complete business formation platform with AI voice calls and automated website builder",
  "private": true,
  "type": "module",
  "main": "server.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "node test-voice-widget.cjs",
    "test:browser": "echo \"Open voice-widget-browser-test.html in your browser\"",
    "test:flows": "./tests/run-tests.sh",
    "test:local": "TEST_URL=http://localhost:3000 ./tests/run-tests.sh",
    "test:qa": "node qa-automation/qa-runner.js",
    "test:qa-smoke": "node qa-automation/qa-runner.js --testSuite smoke",
    "test:qa-regression": "node qa-automation/qa-runner.js --testSuite regression",
    "test:qa-full": "node qa-automation/qa-runner.js --testSuite full",
    "test:qa-headless": "node qa-automation/qa-runner.js --headless true",
    "qa:generate-data": "node -p \"const { QADataGenerator } = require('./qa-automation/qa-data-generator'); const gen = new QADataGenerator(); console.log(JSON.stringify(gen.generateTestSuite('full'), null, 2))\"",
    "qa:user-flows": "node -p \"console.log('User flow automation available - use test:qa to run')\"",
    "qa:dashboard": "node qa-automation/qa-dashboard-integration.js",
    "qa:dashboard-smoke": "node qa-automation/qa-dashboard-integration.js smoke",
    "qa:dashboard-regression": "node qa-automation/qa-dashboard-integration.js regression",
    "qa:dashboard-full": "node qa-automation/qa-dashboard-integration.js full",
    "qa:cli": "node qa-automation/qa-cli.js",
    "qa:add": "node qa-automation/qa-cli.js add",
    "qa:add-feature": "node qa-automation/qa-cli.js add-feature",
    "qa:list": "node qa-automation/qa-cli.js list",
    "qa:detect": "node qa-automation/qa-cli.js detect",
    "qa:watch": "node qa-automation/qa-cli.js watch",
    "db:check": "node check-database-status.js",
    "db:setup": "node setup-complete-database.js",
    "db:verify": "node verify-database-setup.js",
    "nav:update": "node update-navigation.js",
    "nav:analyze": "node analyze-pages.js",
    "nav:check": "echo \"Navigation scripts available: nav:update, nav:analyze\""
  },
  "keywords": [
    "dreamseed",
    "vapi",
    "voice-ai",
    "business-formation",
    "web-calls",
    "website-builder",
    "next.js",
    "supabase",
    "openai",
    "realtime-agents"
  ],
  "author": "DreamSeed Team",
  "license": "MIT",
  "dependencies": {
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/auth-ui-react": "^0.4.7",
    "@supabase/auth-ui-shared": "^0.1.8",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.38.0",
    "@swc/helpers": "^0.5.17",
    "axios": "^1.11.0",
    "dotenv": "^16.3.1",
    "express": "^5.1.0",
    "glob": "^10.4.5",
    "next": "^14.2.0",
    "openai": "^5.12.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dreamseed2025/dreamseed_website_builder.git"
  },
  "homepage": "https://github.com/dreamseed2025/dreamseed_website_builder",
  "bugs": {
    "url": "https://github.com/dreamseed2025/dreamseed_website_builder/issues"
  },
  "features": {
    "website-builder": {
      "enabled": true,
      "port": 3000,
      "templates": [
        "modern-saas-01"
      ],
      "deployment": "vercel"
    },
    "vapi-system": {
      "enabled": true,
      "port": 3002,
      "web-calls": true,
      "phone-callbacks": true,
      "customer-journey": true,
      "monitoring": true,
      "location": "public/simple-vapi-webhook"
    },
    "realtime-voice": {
      "enabled": true,
      "interrupt-handling": true,
      "voice-activity-detection": true,
      "low-latency": true,
      "status": "ready-for-openai-realtime-agents"
    }
  },
  "workspaces": [
    "public/simple-vapi-webhook"
  ],
  "changelog": "CHANGELOG.md",
  "devDependencies": {
    "@faker-js/faker": "^9.9.0",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "jest": "^29.0.0",
    "playwright": "^1.40.0",
    "puppeteer": "^21.0.0",
    "typescript": "^5.9.2"
  }
}
