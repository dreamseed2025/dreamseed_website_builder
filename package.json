{
  "name": "dreamseed-platform",
  "version": "2.0.0",
  "description": "Complete business formation platform with AI voice calls and automated website builder",
  "private": true,
  "main": "server.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "vapi:start": "cd public/simple-vapi-webhook && npm start",
    "vapi:dev": "cd public/simple-vapi-webhook && npm run dev",
    "vapi:install": "cd public/simple-vapi-webhook && npm install",
    "test": "echo \"No tests yet\" && exit 0",
    "version": "echo \"DreamSeed Platform v2.0.0 - Business Formation & Website Builder\""
  },
  "keywords": [
    "dreamseed",
    "vapi",
    "voice-ai",
    "business-formation",
    "web-calls",
    "website-builder",
    "next.js",
    "supabase",
    "openai"
  ],
  "author": "DreamSeed Team",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.55.0",
    "next": "^14.2.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.12",
    "@types/node": "^20.14.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.40",
    "tailwindcss": "^3.4.7",
    "typescript": "^5.5.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dreamseed2025/dreamseed_website_builder.git"
  },
  "homepage": "https://github.com/dreamseed2025/dreamseed_website_builder",
  "bugs": {
    "url": "https://github.com/dreamseed2025/dreamseed_website_builder/issues"
  },
  "features": {
    "website-builder": {
      "enabled": true,
      "port": 3000,
      "templates": ["modern-saas-01"],
      "deployment": "vercel"
    },
    "vapi-system": {
      "enabled": true,
      "port": 3002,
      "web-calls": true,
      "phone-callbacks": true,
      "customer-journey": true,
      "monitoring": true,
      "location": "public/simple-vapi-webhook"
    }
  },
  "workspaces": [
    "public/simple-vapi-webhook"
  ],
  "changelog": "CHANGELOG.md"
}